{
  "name": "basalt-assesment",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "npm": "=8.19.2",
    "node": "=16.18.1"
  },
  "scripts": {
    "server": "node ./dist-server/server.js",
    "seed": "NODE_ENV=Development node ./dist-server/core/mongo/seed/seed.js",
    "transpile": "babel ./server --out-dir dist-server --copy-files",
    "clean": "rimraf dist-server",
    "create": "mkdirp dist-server",
    "build": "npm-run-all create clean transpile",
    "dev": "NODE_ENV=Development npm-run-all build server",
    "prod": "NODE_ENV=Production npm-run-all build server",
    "start": "npm run prod",
    "watch:dev": "nodemon"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-throw-expressions",
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "keywords": [],
  "author": "Callan Brombacher",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-throw-expressions": "^7.18.6",
    "@babel/preset-env": "^7.21.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "mkdirp": "^2.1.6",
    "mongodb": "^5.1.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.1",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "prettier": "^2.8.7"
  }
}
